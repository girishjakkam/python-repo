
import http.client

def printText(txt):
    lines = txt.split('\n')
    for line in lines:
        print (line.strip())

httpServ = http.client.HTTPConnection("www.google.com", 80)
httpServ.connect()

quote = "test"
httpServ.request('POST', '/cgi_form.cgi', 'name=Brad&quote=%s' % quote)

response= httpServ.getresponse()
if response.status == http.client.OK:
    print ("Output from CGI request")
    printText (response.read())
print response.read()
httpServ.close()
